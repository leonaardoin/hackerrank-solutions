function saveThePrisoner(n, m, s) {
 //add s and m subtract 1 from m to determine value
  let value = s + m - 1;
//check if value greater than n
    return (value > n)
    //if greater than n, return remainder of value by n
        ? (!(value % n) ? n : value % n)
        //if less than or equal to n return calcuated value
        : value;
}
